@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <meta name="viewport" content="width=device-width" />
    <title>Gestión de bienes</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/Navbar.css" />
    <link rel="stylesheet" href="~/css/Create.css" />
    <link rel="stylesheet" href="~/css/Login.css" />
    <link href="~/css/Estilos.css" rel="stylesheet" />
</head>

<body>

    <nav class="navbar navbar-expand-lg bg-secondary text-uppercase fixed-top" id="mainNav">
        <div class="container" style="display: flex;justify-content: center; align-items: center">
            <a class="navbar-brand js-scroll-trigger" style=" color: #fff;" href="#page-top">Gestion de Bienes</a>
        </div>
    </nav>


    <div class="limiter">
        <div class="container-login100">
            <div class="login100-more" style="background-image: url('../images/cajamarca.jpg');"></div>

            <div class="wrap-login100 p-l-50 p-r-50 p-t-72 p-b-50">
                <br /><br />
                <h1 class="h3 mb-3 font-weight-normal" style="text-align: center"> Registrarse</h1>
                <div>
                    <form action="/auth/create" method="post">
                        <label><b>Nombres:</b></label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="Nombres" placeholder="Ingrese nombres" />
                        </div>
                        <label><b>Apellidos:</b></label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="Apellidos" placeholder="Ingrese apellidos" />
                        </div>
                        <label><b>Nombre de Usuario:</b></label>
                        <div class="form-group">
                            <input type="text" class="form-control" name="Username" placeholder="Ingrese nombre de usuario" />
                        </div>
                        <label><b>Contraseña:</b></label>
                        <div class="form-group">
                            <input type="password" class="form-control" name="Password" placeholder="Ingrese contraseña" />
                        </div><br />

                              <div class="form-group" style="display: flex; align-items: flex-end">
                                  <button class="btn btn-lg btn-primary btn-block btn-signin" style="margin-right: 10px" type="submit">Registrarse</button>
                                  <a class="btn btn-lg btn-primary btn-block btn-signin" style="margin-right: 10px; padding-top: 6px" href="/Home/Index">Cancelar</a>
                              </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
</body>
</html>
